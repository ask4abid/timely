{
  "name": "timely-world-clock",
  "version": "2.0.0",
  "description": "Your ultimate world clock and timezone companion with real-time updates",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "start": "npx http-server . -p 3000 -o",
    "dev": "npx live-server --port=3000 --open=index.html",
    "serve": "npx serve . -p 3000 -s",
    "preview": "npx http-server . -p 8080 -o",
    "test": "npx jest"
  },
  "keywords": ["world clock", "timezone", "time zones", "real-time", "javascript", "modular"],
  "author": "Hafiz Muhammad Abid",
  "license": "MIT",
  "devDependencies": {
    "http-server": "^14.1.1",
    "live-server": "^1.2.2",
    "serve": "^14.2.1"
  }
}
